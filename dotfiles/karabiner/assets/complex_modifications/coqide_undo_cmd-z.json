{
  "title": "Undo in CoqIDE with Cmd+Z",
  "rules": [
    {
      "description": "Undo in CoqIDE with Cmd+Z",
      "manipulators": [
        {
          "conditions": [
            {
              "bundle_identifiers": [
                "^fr\\.inria\\.coq\\.coqide$"
              ],
              "type": "frontmost_application_if"
            }
          ],
          "from": {
            "key_code": "z",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "u",
              "modifiers": [
                "left_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
